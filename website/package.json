{
	"name": "teeline.online",
	"version": "0.0.1",
	"scripts": {
		"dev": "npm run format && npm run lint && npm run copy-data && vite dev",
		"build": "npm run format && npm run lint && npm run copy-data && vite build",
		"preview": "npm run copy-data && vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"copy-data": "mkdir static/data && cp src/data/outlines.json static/data/outlines.json"
	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^2.0.7",
		"@sveltejs/kit": "1.22.1",
		"@tsconfig/svelte": "^5.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.4",
		"mdsvex": "^0.11.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.0.5",
		"svelte-check": "^3.5.0",
		"svelte-preprocess": "^5.0.4",
		"typescript": "~5.1.6"
	},
	"type": "module",
	"dependencies": {
		"vite": "^4.4.2"
	}
}
