{
	"name": "teeline.online",
	"version": "0.0.1",
	"scripts": {
		"dev": "npm run copy-data && vite dev",
		"build": "npm run copy-data && vite build",
		"preview": "npm run copy-data && vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"copy-data": "mkdir static/data && cp src/data/outlines.json static/data/outlines.json"
	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^2.0.6",
		"@sveltejs/kit": "1.15.8",
		"@tsconfig/svelte": "^4.0.1",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"mdsvex": "^0.10.6",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"svelte-preprocess": "^5.0.3",
		"typescript": "~5.0.4"
	},
	"type": "module",
	"dependencies": {
		"vite": "^4.3.2"
	}
}